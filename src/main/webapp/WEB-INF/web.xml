<!DOCTYPE  web-app
		PUBLIC
		"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
		"http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>
			index.jsp
		</welcome-file>
	</welcome-file-list>
	<!--
	 -->
	<servlet>
		<servlet-name>QuartzInitializer</servlet-name>
		<servlet-class>
			org.quartz.ee.servlet.QuartzInitializerServlet
		</servlet-class>
		<init-param>
			<param-name>config-file</param-name>
			<param-value>/quartz.properties</param-value>
		</init-param>
		<init-param>
			<param-name>shutdown-on-unload</param-name>
			<param-value>true</param-value>
		</init-param>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<listener>
		<listener-class>
			org.apache.commons.fileupload.servlet.FileCleanerCleanup
		</listener-class>
	</listener>
	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>com.diaodu.controller.IndexServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>batch</servlet-name>
		<servlet-class>com.diaodu.controller.BatchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>batch</servlet-name>
		<url-pattern>/batch</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>task</servlet-name>
		<servlet-class>com.diaodu.controller.TaskServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>task</servlet-name>
		<url-pattern>/task</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>history</servlet-name>
		<servlet-class>com.diaodu.controller.HistoryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>history</servlet-name>
		<url-pattern>/history</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>current</servlet-name>
		<servlet-class>com.diaodu.controller.CurrentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>current</servlet-name>
		<url-pattern>/current</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>email</servlet-name>
		<servlet-class>com.diaodu.controller.MailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>email</servlet-name>
		<url-pattern>/email</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ob</servlet-name>
		<servlet-class>com.diaodu.controller.ObserverServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ob</servlet-name>
		<url-pattern>/ob</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>relation</servlet-name>
		<servlet-class>com.diaodu.controller.RelationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>relation</servlet-name>
		<url-pattern>/relation</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>util</servlet-name>
		<servlet-class>com.diaodu.controller.UtilServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>util</servlet-name>
		<url-pattern>/util</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>db</servlet-name>
		<servlet-class>com.diaodu.controller.DBServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>db</servlet-name>
		<url-pattern>/db</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>monitor</servlet-name>
		<servlet-class>com.diaodu.controller.MonitorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>monitor</servlet-name>
		<url-pattern>/monitor</url-pattern>
	</servlet-mapping>




</web-app>
